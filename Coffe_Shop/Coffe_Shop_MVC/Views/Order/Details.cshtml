
@model Order
@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>
<div class="card" style="width: 18rem;">
    <div class="card-header"> Details of table </div>
    <ul class="list-group list-group-flush">
        <li class="list-group-item">Number of order: @Model.Order_Number</li>
        <li class="list-group-item">Time of order: @Model.Ordered_At</li>
    </ul>
    <a class="btn btn-primary" asp-action="Index">Return To main page</a>
</div>


